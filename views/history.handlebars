        <div class="table-data" id="sets-2">3</div>
        <div class="table-data" id="reps-2">2</div>
        <div class="table-data" id="weights-2">15</div>
      </div>
      <div class="table-row" id="row-3">
        <div class="table-data" id="name-3">Leg press</div>
        <div class="table-data" id="date-3">16/04/2022</div>
        <div class="table-data" id="sets-3">3</div>
        <div class="table-data" id="reps-3">2</div>
        <div class="table-data" id="weights-3">15</div>
      </div>
    </div>
  </div>
</div>

